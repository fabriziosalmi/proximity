{
  "id": "audiobookshelf",
  "name": "Audiobookshelf",
  "description": "Self-hosted audiobook and podcast server",
  "version": "latest",
  "icon": "https://raw.githubusercontent.com/advplyr/audiobookshelf/master/client/static/Logo.png",
  "category": "Media",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "audiobookshelf": {
        "image": "ghcr.io/advplyr/audiobookshelf:latest",
        "ports": ["13378:80"],
        "environment": {
          "TZ": "UTC"
        },
        "volumes": [
          "audiobooks:/audiobooks",
          "podcasts:/podcasts",
          "config:/config",
          "metadata:/metadata"
        ]
      }
    },
    "volumes": {
      "audiobooks": {},
      "podcasts": {},
      "config": {},
      "metadata": {}
    }
  },
  "ports": [13378],
  "volumes": ["audiobooks", "podcasts", "config", "metadata"],
  "environment": {
    "TZ": "UTC"
  },
  "min_memory": 512,
  "min_cpu": 1,
  "tags": ["audiobook", "podcast", "media", "library"],
  "author": "advplyr",
  "website": "https://www.audiobookshelf.org"
}
