{
  "id": "gotify",
  "name": "Gotify",
  "description": "Simple server for sending and receiving messages",
  "version": "latest",
  "icon": "https://raw.githubusercontent.com/gotify/logo/master/gotify-logo.png",
  "category": "Communication",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "gotify": {
        "image": "gotify/server:latest",
        "environment": {
          "GOTIFY_DEFAULTUSER_PASS": "admin",
          "TZ": "UTC"
        },
        "volumes": [
          "data:/app/data"
        ],
        "network_mode": "host"
      }
    },
    "volumes": {
      "data": {}
    }
  },
  "ports": [
    8080
  ],
  "volumes": [
    "data"
  ],
  "environment": {
    "GOTIFY_DEFAULTUSER_PASS": "admin",
    "TZ": "UTC"
  },
  "min_memory": 128,
  "min_cpu": 1,
  "tags": [
    "notifications",
    "messaging",
    "push"
  ],
  "author": "Gotify",
  "website": "https://gotify.net"
}