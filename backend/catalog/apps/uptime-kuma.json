{
  "id": "uptime-kuma",
  "name": "Uptime Kuma",
  "description": "Self-hosted uptime monitoring tool with beautiful dashboard",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/uptimekuma/5CDD8B",
  "category": "Monitoring",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "uptime-kuma": {
        "image": "louislam/uptime-kuma:latest",
        "volumes": [
          "uptime_data:/app/data"
        ],
        "restart": "always",
        "network_mode": "host"
      }
    },
    "volumes": {
      "uptime_data": {}
    }
  },
  "ports": [
    3001
  ],
  "volumes": [
    "uptime_data"
  ],
  "environment": {},
  "min_memory": 256,
  "min_cpu": 1,
  "tags": [
    "monitoring",
    "uptime",
    "status-page",
    "notifications"
  ],
  "author": "Louis Lam",
  "website": "https://uptime.kuma.pet"
}