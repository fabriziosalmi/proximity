{
  "id": "dashy",
  "name": "Dashy",
  "description": "Feature-rich homepage for your homelab with customizable widgets, themes, and status monitoring",
  "category": "Automation",
  "icon": "https://cdn.simpleicons.org/webcomponentsdotorg/29ABE2",
  "tags": ["dashboard", "homepage", "widgets", "monitoring", "homelab"],
  "min_memory": 256,
  "min_cpu": 1,
  "min_disk": 1,
  "docker_compose": {
    "version": "3.8",
    "services": {
      "dashy": {
        "image": "lissy93/dashy:latest",
        "container_name": "dashy",
        "restart": "unless-stopped",
        "ports": [
          "4000:80"
        ],
        "volumes": [
          "dashy_config:/app/public"
        ],
        "environment": {
          "NODE_ENV": "production"
        }
      }
    },
    "volumes": {
      "dashy_config": {}
    }
  }
}
