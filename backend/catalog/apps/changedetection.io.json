{
  "id": "changedetection.io",
  "name": "Changedetection.io",
  "description": "The best and simplest self-hosted free open source website change detection, restock monitor and notification service.",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/changedetection/F57F29",
  "category": "Utilities",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "changedetection": {
        "image": "dgtlmoon/changedetection.io:latest",
        "restart": "unless-stopped",
        "ports": [
          "5000:5000"
        ],
        "volumes": [
          "changedetection_data:/datastore"
        ]
      }
    },
    "volumes": {
      "changedetection_data": null
    }
  }
}
