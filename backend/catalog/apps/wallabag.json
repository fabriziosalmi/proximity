{
  "id": "wallabag",
  "name": "Wallabag",
  "description": "Self-hosted read-it-later application",
  "version": "latest",
  "icon": "https://raw.githubusercontent.com/wallabag/logo/master/_default/typo-horizontal/png/sm/logo-typo-horizontal-black-no-bg-no-border-sm.png",
  "category": "Productivity",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "wallabag": {
        "image": "wallabag/wallabag:latest",
        "environment": {
          "SYMFONY__ENV__DATABASE_DRIVER": "pdo_pgsql",
          "SYMFONY__ENV__DATABASE_HOST": "db",
          "SYMFONY__ENV__DATABASE_PORT": "5432",
          "SYMFONY__ENV__DATABASE_NAME": "wallabag",
          "SYMFONY__ENV__DATABASE_USER": "wallabag",
          "SYMFONY__ENV__DATABASE_PASSWORD": "wallabag_password",
          "SYMFONY__ENV__DOMAIN_NAME": "http://localhost"
        },
        "volumes": [
          "images:/var/www/wallabag/web/assets/images"
        ],
        "depends_on": [
          "db"
        ],
        "network_mode": "host"
      },
      "db": {
        "image": "postgres:15-alpine",
        "environment": {
          "POSTGRES_DB": "wallabag",
          "POSTGRES_USER": "wallabag",
          "POSTGRES_PASSWORD": "wallabag_password"
        },
        "volumes": [
          "db_data:/var/lib/postgresql/data"
        ],
        "network_mode": "host"
      }
    },
    "volumes": {
      "images": {},
      "db_data": {}
    }
  },
  "ports": [
    80
  ],
  "volumes": [
    "images",
    "db_data"
  ],
  "environment": {
    "SYMFONY__ENV__DATABASE_PASSWORD": "wallabag_password",
    "SYMFONY__ENV__DOMAIN_NAME": "http://localhost"
  },
  "min_memory": 512,
  "min_cpu": 1,
  "tags": [
    "reading",
    "bookmarks",
    "productivity"
  ],
  "author": "Wallabag",
  "website": "https://wallabag.org"
}