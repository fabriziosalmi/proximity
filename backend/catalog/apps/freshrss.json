{
  "id": "freshrss",
  "name": "FreshRSS",
  "description": "Free, self-hostable RSS aggregator with multi-user support and mobile apps",
  "category": "Communication",
  "icon": "https://cdn.simpleicons.org/rss/FFA500",
  "tags": ["rss", "feed", "news", "aggregator", "reader"],
  "min_memory": 256,
  "min_cpu": 1,
  "min_disk": 2,
  "docker_compose": {
    "version": "3.8",
    "services": {
      "freshrss": {
        "image": "lscr.io/linuxserver/freshrss:latest",
        "container_name": "freshrss",
        "restart": "unless-stopped",
        "environment": {
          "PUID": "1000",
          "PGID": "1000",
          "TZ": "Etc/UTC"
        },
        "volumes": [
          "freshrss_config:/config"
        ],
        "ports": [
          "8080:80"
        ]
      }
    },
    "volumes": {
      "freshrss_config": {}
    }
  }
}
