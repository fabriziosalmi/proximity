{
  "id": "seafile",
  "name": "Seafile",
  "description": "High-performance file sync and share solution",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/files/00A4DC",
  "category": "Storage",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "seafile": {
        "image": "seafileltd/seafile-mc:latest",
        "environment": {
          "DB_HOST": "db",
          "DB_ROOT_PASSWD": "root_password",
          "SEAFILE_ADMIN_EMAIL": "admin@seafile.local",
          "SEAFILE_ADMIN_PASSWORD": "admin",
          "SEAFILE_SERVER_HOSTNAME": "localhost"
        },
        "volumes": [
          "seafile_data:/shared"
        ],
        "depends_on": [
          "db"
        ],
        "restart": "always",
        "network_mode": "host"
      },
      "db": {
        "image": "mariadb:10.11",
        "environment": {
          "MYSQL_ROOT_PASSWORD": "root_password"
        },
        "volumes": [
          "seafile_db:/var/lib/mysql"
        ],
        "restart": "always",
        "network_mode": "host"
      }
    },
    "volumes": {
      "seafile_data": {},
      "seafile_db": {}
    }
  },
  "ports": [
    80,
    443
  ],
  "volumes": [
    "seafile_data",
    "seafile_db"
  ],
  "environment": {
    "SEAFILE_ADMIN_PASSWORD": "admin",
    "DB_ROOT_PASSWD": "root_password"
  },
  "min_memory": 2048,
  "min_cpu": 2,
  "tags": [
    "storage",
    "sync",
    "collaboration",
    "cloud"
  ],
  "author": "Seafile Ltd",
  "website": "https://www.seafile.com"
}