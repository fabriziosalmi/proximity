{
  "id": "jellyseerr",
  "name": "Jellyseerr",
  "description": "Media request management and discovery tool for Jellyfin, Plex, and Emby",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/jellyfin/00A4DC",
  "category": "Media",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "jellyseerr": {
        "image": "fallenbagel/jellyseerr:latest",
        "restart": "unless-stopped",
        "ports": [
          "5055:5055"
        ],
        "volumes": [
          "jellyseerr_config:/app/config"
        ],
        "environment": {
          "LOG_LEVEL": "info",
          "TZ": "UTC"
        }
      }
    },
    "volumes": {
      "jellyseerr_config": null
    }
  }
}
