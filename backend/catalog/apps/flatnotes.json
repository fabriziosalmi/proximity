{
  "id": "flatnotes",
  "name": "Flatnotes",
  "description": "Self-hosted, database-less note taking web app",
  "version": "latest",
  "icon": "https://raw.githubusercontent.com/dullage/flatnotes/main/client/public/favicon.svg",
  "category": "Productivity",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "flatnotes": {
        "image": "dullage/flatnotes:latest",
        "environment": {
          "FLATNOTES_AUTH_TYPE": "password",
          "FLATNOTES_USERNAME": "admin",
          "FLATNOTES_PASSWORD": "change_me_password",
          "FLATNOTES_SECRET_KEY": "change_me_secret_key"
        },
        "volumes": [
          "data:/data"
        ],
        "network_mode": "host"
      }
    },
    "volumes": {
      "data": {}
    }
  },
  "ports": [
    8080
  ],
  "volumes": [
    "data"
  ],
  "environment": {
    "FLATNOTES_USERNAME": "admin",
    "FLATNOTES_PASSWORD": "change_me_password",
    "FLATNOTES_SECRET_KEY": "change_me_secret_key"
  },
  "min_memory": 256,
  "min_cpu": 1,
  "tags": [
    "notes",
    "markdown",
    "productivity"
  ],
  "author": "Dullage",
  "website": "https://flatnotes.io"
}