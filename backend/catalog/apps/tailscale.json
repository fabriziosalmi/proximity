{
  "id": "tailscale",
  "name": "Tailscale",
  "description": "Zero-config VPN built on WireGuard",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/tailscale/000000",
  "category": "Networking",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "tailscale": {
        "image": "tailscale/tailscale:latest",
        "network_mode": "host",
        "cap_add": ["NET_ADMIN", "NET_RAW"],
        "environment": {
          "TS_AUTHKEY": "tskey-auth-change-me",
          "TS_STATE_DIR": "/var/lib/tailscale",
          "TS_EXTRA_ARGS": "--advertise-exit-node"
        },
        "volumes": [
          "state:/var/lib/tailscale",
          "/dev/net/tun:/dev/net/tun"
        ]
      }
    },
    "volumes": {
      "state": {}
    }
  },
  "ports": [],
  "volumes": ["state"],
  "environment": {
    "TS_AUTHKEY": "tskey-auth-change-me"
  },
  "min_memory": 128,
  "min_cpu": 1,
  "tags": ["vpn", "networking", "security", "wireguard"],
  "author": "Tailscale",
  "website": "https://tailscale.com"
}
