{
  "id": "olivetin",
  "name": "OliveTin",
  "description": "Gives safe and simple access to predefined shell commands from a web interface",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/terminal/000000",
  "category": "DevOps",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "olivetin": {
        "image": "jamesread/olivetin:latest",
        "restart": "unless-stopped",
        "ports": [
          "1337:1337"
        ],
        "volumes": [
          "olivetin_config:/etc/OliveTin"
        ],
        "environment": {
          "DISPLAY": ""
        }
      }
    },
    "volumes": {
      "olivetin_config": null
    }
  }
}
