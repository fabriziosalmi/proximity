{
  "id": "portainer",
  "name": "Portainer",
  "description": "Container management platform",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/portainer/13BEF9",
  "category": "DevOps",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "portainer": {
        "image": "portainer/portainer-ce:latest",
        "volumes": [
          "/var/run/docker.sock:/var/run/docker.sock",
          "portainer_data:/data"
        ],
        "restart": "always",
        "network_mode": "host"
      }
    },
    "volumes": {
      "portainer_data": {}
    }
  },
  "ports": [
    9000
  ],
  "volumes": [
    "portainer_data"
  ],
  "environment": {},
  "min_memory": 512,
  "min_cpu": 1,
  "tags": [
    "docker",
    "container",
    "management",
    "devops"
  ],
  "author": "Portainer.io",
  "website": "https://www.portainer.io"
}