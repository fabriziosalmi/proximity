{
  "id": "homarr",
  "name": "Homarr",
  "description": "A simple and lightweight homepage for your server that helps you easily access all of your services in one place.",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/homarr/2B3137",
  "category": "Productivity",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "homarr": {
        "image": "ghcr.io/autobrr/homarr:latest",
        "restart": "unless-stopped",
        "ports": [
          "7575:7575"
        ],
        "volumes": [
          "homarr_configs:/app/data/configs",
          "homarr_icons:/app/public/icons"
        ]
      }
    },
    "volumes": {
      "homarr_configs": null,
      "homarr_icons": null
    }
  }
}
