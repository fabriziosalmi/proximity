{
  "id": "photoprism",
  "name": "PhotoPrism",
  "description": "AI-powered photo management and browsing",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/photoprism/18BAB3",
  "category": "Media",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "photoprism": {
        "image": "photoprism/photoprism:latest",
        "environment": {
          "PHOTOPRISM_ADMIN_PASSWORD": "admin",
          "PHOTOPRISM_DATABASE_DRIVER": "mysql",
          "PHOTOPRISM_DATABASE_SERVER": "db:3306",
          "PHOTOPRISM_DATABASE_NAME": "photoprism",
          "PHOTOPRISM_DATABASE_USER": "photoprism",
          "PHOTOPRISM_DATABASE_PASSWORD": "photoprism_password"
        },
        "volumes": [
          "photoprism_originals:/photoprism/originals",
          "photoprism_storage:/photoprism/storage"
        ],
        "depends_on": [
          "db"
        ],
        "restart": "always",
        "network_mode": "host"
      },
      "db": {
        "image": "mariadb:11",
        "environment": {
          "MYSQL_ROOT_PASSWORD": "root_password",
          "MYSQL_DATABASE": "photoprism",
          "MYSQL_USER": "photoprism",
          "MYSQL_PASSWORD": "photoprism_password"
        },
        "volumes": [
          "photoprism_db:/var/lib/mysql"
        ],
        "restart": "always",
        "network_mode": "host"
      }
    },
    "volumes": {
      "photoprism_originals": {},
      "photoprism_storage": {},
      "photoprism_db": {}
    }
  },
  "ports": [
    2342
  ],
  "volumes": [
    "photoprism_originals",
    "photoprism_storage",
    "photoprism_db"
  ],
  "environment": {
    "PHOTOPRISM_ADMIN_PASSWORD": "admin",
    "MYSQL_PASSWORD": "photoprism_password"
  },
  "min_memory": 2048,
  "min_cpu": 2,
  "tags": [
    "photos",
    "media",
    "ai",
    "gallery"
  ],
  "author": "PhotoPrism",
  "website": "https://photoprism.app"
}