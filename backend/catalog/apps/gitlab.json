{
  "id": "gitlab",
  "name": "GitLab CE",
  "description": "Complete DevOps platform with Git repository management",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/gitlab/FC6D26",
  "category": "DevOps",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "gitlab": {
        "image": "gitlab/gitlab-ce:latest",
        "ports": ["80:80", "443:443", "22:22"],
        "environment": {
          "GITLAB_OMNIBUS_CONFIG": "external_url 'http://localhost'"
        },
        "volumes": [
          "gitlab_config:/etc/gitlab",
          "gitlab_logs:/var/log/gitlab",
          "gitlab_data:/var/opt/gitlab"
        ],
        "restart": "always"
      }
    },
    "volumes": {
      "gitlab_config": {},
      "gitlab_logs": {},
      "gitlab_data": {}
    }
  },
  "ports": [80, 443, 22],
  "volumes": ["gitlab_config", "gitlab_logs", "gitlab_data"],
  "environment": {},
  "min_memory": 4096,
  "min_cpu": 4,
  "tags": ["git", "devops", "ci-cd", "repository"],
  "author": "GitLab Inc",
  "website": "https://about.gitlab.com"
}
