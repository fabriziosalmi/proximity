{
  "id": "nginx",
  "name": "Nginx",
  "description": "High-performance web server and reverse proxy",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/nginx/009639",
  "category": "Web Server",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "nginx": {
        "image": "nginx:latest",
        "network_mode": "host",
        "volumes": [
          "nginx_data:/usr/share/nginx/html",
          "nginx_config:/etc/nginx/conf.d"
        ],
        "restart": "always"
      }
    },
    "volumes": {
      "nginx_data": {},
      "nginx_config": {}
    }
  },
  "ports": [80, 443],
  "volumes": ["nginx_data", "nginx_config"],
  "environment": {},
  "min_memory": 256,
  "min_cpu": 1,
  "tags": ["web-server", "proxy", "http"],
  "author": "Nginx Inc.",
  "website": "https://nginx.org"
}
