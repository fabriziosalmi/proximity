{
  "id": "element",
  "name": "Element",
  "description": "Secure and decentralized collaboration with Matrix protocol - chat, voice, video, file sharing",
  "category": "Communication",
  "icon": "https://cdn.simpleicons.org/element/0DBD8B",
  "tags": [
    "chat",
    "matrix",
    "messaging",
    "collaboration",
    "voice",
    "video"
  ],
  "min_memory": 512,
  "min_cpu": 1,
  "min_disk": 2,
  "docker_compose": {
    "version": "3.8",
    "services": {
      "element": {
        "image": "vectorim/element-web:latest",
        "container_name": "element",
        "restart": "unless-stopped",
        "volumes": [
          "element_config:/app/config"
        ],
        "environment": {
          "MATRIX_SERVER": "https://matrix.org"
        },
        "network_mode": "host"
      }
    },
    "volumes": {
      "element_config": {}
    }
  },
  "version": "latest"
}