{
  "id": "minio",
  "name": "MinIO",
  "description": "High-performance S3-compatible object storage",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/minio/C72E49",
  "category": "Storage",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "minio": {
        "image": "minio/minio:latest",
        "environment": {
          "MINIO_ROOT_USER": "admin",
          "MINIO_ROOT_PASSWORD": "admin_password"
        },
        "command": "server /data --console-address ':9001'",
        "volumes": [
          "minio_data:/data"
        ],
        "restart": "always",
        "network_mode": "host"
      }
    },
    "volumes": {
      "minio_data": {}
    }
  },
  "ports": [
    9000,
    9001
  ],
  "volumes": [
    "minio_data"
  ],
  "environment": {
    "MINIO_ROOT_PASSWORD": "admin_password"
  },
  "min_memory": 512,
  "min_cpu": 1,
  "tags": [
    "storage",
    "s3",
    "object-storage",
    "backup"
  ],
  "author": "MinIO Inc",
  "website": "https://min.io"
}