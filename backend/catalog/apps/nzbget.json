{
  "id": "nzbget",
  "name": "NZBGet",
  "description": "Efficient binary newsgrabber for Usenet",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/nzbget/009688",
  "category": "Utilities",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "nzbget": {
        "image": "linuxserver/nzbget:latest",
        "restart": "unless-stopped",
        "ports": [
          "6789:6789"
        ],
        "volumes": [
          "nzbget_config:/config",
          "nzbget_downloads:/downloads"
        ],
        "environment": {
          "PUID": "1000",
          "PGID": "1000",
          "TZ": "UTC"
        }
      }
    },
    "volumes": {
      "nzbget_config": null,
      "nzbget_downloads": null
    }
  }
}
