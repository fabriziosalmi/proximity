{
  "id": "heimdall",
  "name": "Heimdall",
  "description": "Application dashboard and launcher with a clean, minimal interface for organizing your services",
  "category": "Automation",
  "icon": "https://cdn.simpleicons.org/homeassistant/18BCF2",
  "tags": [
    "dashboard",
    "launcher",
    "homepage",
    "organization",
    "bookmarks"
  ],
  "min_memory": 128,
  "min_cpu": 1,
  "min_disk": 1,
  "docker_compose": {
    "version": "3.8",
    "services": {
      "heimdall": {
        "image": "lscr.io/linuxserver/heimdall:latest",
        "container_name": "heimdall",
        "restart": "unless-stopped",
        "environment": {
          "PUID": "1000",
          "PGID": "1000",
          "TZ": "Etc/UTC"
        },
        "volumes": [
          "heimdall_config:/config"
        ],
        "ports": [
          "8080:80",
          "8443:443"
        ]
      }
    },
    "volumes": {
      "heimdall_config": {}
    }
  },
  "version": "latest"
}
