{
  "id": "traefik",
  "name": "Traefik",
  "description": "Modern HTTP reverse proxy and load balancer",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/traefikproxy/24A1C1",
  "category": "Web Server",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "traefik": {
        "image": "traefik:latest",
        "ports": ["80:80", "443:443", "8080:8080"],
        "command": [
          "--api.dashboard=true",
          "--api.insecure=true",
          "--providers.docker=true",
          "--entrypoints.web.address=:80",
          "--entrypoints.websecure.address=:443"
        ],
        "volumes": [
          "/var/run/docker.sock:/var/run/docker.sock:ro",
          "traefik_data:/letsencrypt"
        ],
        "restart": "always"
      }
    },
    "volumes": {
      "traefik_data": {}
    }
  },
  "ports": [80, 443, 8080],
  "volumes": ["traefik_data"],
  "environment": {},
  "min_memory": 256,
  "min_cpu": 1,
  "tags": ["reverse-proxy", "load-balancer", "web-server", "routing"],
  "author": "Traefik Labs",
  "website": "https://traefik.io"
}
