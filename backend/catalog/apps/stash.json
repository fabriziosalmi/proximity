{
  "id": "stash",
  "name": "Stash",
  "description": "An organizer for your porn, written in Go.",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/stash/E91E63",
  "category": "Media",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "stash": {
        "image": "stashapp/stash:latest",
        "restart": "unless-stopped",
        "ports": [
          "9999:9999"
        ],
        "volumes": [
          "stash_config:/root/.stash",
          "stash_data:/data"
        ]
      }
    },
    "volumes": {
      "stash_config": null,
      "stash_data": null
    }
  }
}
