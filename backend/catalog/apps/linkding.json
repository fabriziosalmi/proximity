{
  "id": "linkding",
  "name": "linkding",
  "description": "Self-hosted bookmark manager with minimal UI",
  "version": "latest",
  "icon": "https://raw.githubusercontent.com/sissbruecker/linkding/master/docs/linkding-logo.svg",
  "category": "Productivity",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "linkding": {
        "image": "sissbruecker/linkding:latest",
        "volumes": [
          "data:/etc/linkding/data"
        ],
        "environment": {
          "LD_SUPERUSER_NAME": "admin",
          "LD_SUPERUSER_PASSWORD": "change_me_password"
        },
        "network_mode": "host"
      }
    },
    "volumes": {
      "data": {}
    }
  },
  "ports": [
    9090
  ],
  "volumes": [
    "data"
  ],
  "environment": {
    "LD_SUPERUSER_NAME": "admin",
    "LD_SUPERUSER_PASSWORD": "change_me_password"
  },
  "min_memory": 256,
  "min_cpu": 1,
  "tags": [
    "bookmarks",
    "productivity",
    "organization"
  ],
  "author": "sissbruecker",
  "website": "https://github.com/sissbruecker/linkding"
}