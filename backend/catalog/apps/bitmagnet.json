{
  "id": "bitmagnet",
  "name": "Bitmagnet",
  "description": "Self-hosted BitTorrent indexer, DHT crawler and content classifier",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/torrent/E41D1F",
  "category": "Media",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "bitmagnet": {
        "image": "bitmagnetio/bitmagnet:latest",
        "restart": "unless-stopped",
        "ports": [
          "3333:3333",
          "4444:4444/tcp",
          "4444:4444/udp"
        ],
        "volumes": [
          "bitmagnet_data:/data"
        ],
        "environment": {
          "POSTGRES_HOST": "bitmagnet_postgres",
          "POSTGRES_PORT": "5432",
          "POSTGRES_DB": "bitmagnet",
          "POSTGRES_USER": "bitmagnet",
          "POSTGRES_PASSWORD": "bitmagnet",
          "DHT_ENABLED": "true"
        },
        "depends_on": [
          "bitmagnet_postgres"
        ]
      },
      "bitmagnet_postgres": {
        "image": "postgres:15-alpine",
        "environment": {
          "POSTGRES_DB": "bitmagnet",
          "POSTGRES_USER": "bitmagnet",
          "POSTGRES_PASSWORD": "bitmagnet"
        },
        "volumes": [
          "bitmagnet_postgres_data:/var/lib/postgresql/data"
        ]
      }
    },
    "volumes": {
      "bitmagnet_data": null,
      "bitmagnet_postgres_data": null
    }
  }
}
