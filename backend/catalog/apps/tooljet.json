{
  "id": "tooljet",
  "name": "ToolJet",
  "description": "Low-code platform for building internal tools",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/tooljet/3F51B5",
  "category": "Development",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "tooljet": {
        "image": "tooljet/tooljet-ce:latest",
        "restart": "unless-stopped",
        "ports": [
          "3000:3000"
        ],
        "environment": {
          "NODE_ENV": "production",
          "TOOLJET_HOST": "http://localhost:3000",
          "SECRET_KEY_BASE": "change_me_secret",
          "LOCKBOX_MASTER_KEY": "change_me_master",
          "TOOLJET_DB_TYPE": "postgres",
          "TOOLJET_DB_HOST": "tooljet_db",
          "TOOLJET_DB_PORT": "5432",
          "TOOLJET_DB_NAME": "tooljet",
          "TOOLJET_DB_USER": "tooljet",
          "TOOLJET_DB_PASS": "tooljet",
          "REDIS_HOST": "tooljet_redis",
          "REDIS_PORT": "6379"
        },
        "depends_on": [
          "tooljet_db",
          "tooljet_redis"
        ]
      },
      "tooljet_db": {
        "image": "postgres:15-alpine",
        "environment": {
          "POSTGRES_DB": "tooljet",
          "POSTGRES_USER": "tooljet",
          "POSTGRES_PASSWORD": "tooljet"
        },
        "volumes": [
          "tooljet_db_data:/var/lib/postgresql/data"
        ]
      },
      "tooljet_redis": {
        "image": "redis:7-alpine",
        "command": "redis-server --appendonly yes",
        "volumes": [
          "tooljet_redis_data:/data"
        ]
      }
    },
    "volumes": {
      "tooljet_db_data": null,
      "tooljet_redis_data": null
    }
  }
}
