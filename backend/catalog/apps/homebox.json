{
  "id": "homebox",
  "name": "Homebox",
  "description": "Inventory and organization system for home users",
  "version": "latest",
  "icon": "https://cdn.simpleicons.org/box/6366F1",
  "category": "Productivity",
  "docker_compose": {
    "version": "3.8",
    "services": {
      "homebox": {
        "image": "sysadminsmedia/homebox:latest",
        "restart": "unless-stopped",
        "ports": [
          "5173:5173"
        ],
        "volumes": [
          "homebox_data:/root/.local/share/homebox"
        ],
        "environment": {
          "HBOX_LOG_LEVEL": "info"
        }
      }
    },
    "volumes": {
      "homebox_data": null
    }
  }
}
